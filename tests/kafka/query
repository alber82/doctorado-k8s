from(bucket: "doctorado")                                                            
  |> range(start: -1m)
  |> filter(fn: (r) => r["_measurement"] == "prometheus_remote_write")
  |> filter(fn: (r) => r["_field"] == "node_network_transmit_bytes_total")
  |> group(columns: [ "instance"], mode:"by")
  |> last()
