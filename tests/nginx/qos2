
influx query  'import "math"
First = from(bucket: "doctorado")
  |> range(start: -15m, stop: now())
  |> filter(fn: (r) => r["_measurement"] == "node_network_receive_bytes_total")
  |> filter(fn: (r) => r["device"] == "eth0")
  |> group(columns: ["url"], mode:"by")
  |> keep(columns: ["url", "_value"])
  |> first()

Last = from(bucket: "doctorado")
  |> range(start: -15m, stop: now())
  |> filter(fn: (r) => r["_measurement"] == "node_network_receive_bytes_total")
  |> filter(fn: (r) => r["device"] == "eth0")
  |> group(columns: [ "url"], mode:"by")
  |> keep(columns: ["url", "_value"])
  |> last()

union(tables: [ First, Last])
|> difference()
|> map(fn: (r) => ({r with _value: math.abs(x: r._value)}))'


import "math"
First = from(bucket: "doctorado")
	|> range(start: -10m, stop: now())
	|> filter(fn: (r) => r["_measurement"] == "node_network_receive_bytes_total")
	|> filter(fn: (r) => r["device"] == "eth0")
	|> group(columns: ["url"], mode:"by")
	|> keep(columns: ["url", "_value"])
	|> first()


Last = from(bucket: "doctorado")
	|> range(start: -10m, stop: now())
	|> filter(fn: (r) => r["_measurement"] == "node_network_receive_bytes_total")
	|> filter(fn: (r) => r["device"] == "eth0")
	|> group(columns: ["url"], mode:"by")
	|> keep(columns: ["url", "_value"])
	|> last()

	union(tables: [First, Last])
	|> difference()
	|> map(fn: (r) => ({r with _value: math.abs(x: float(v: r._value))}))
    |> yield(name: "delta_read_time")







influx query  'import "math"
First = from(bucket: "doctorado")
  |> range(start: -5m, stop: now())
  |> filter(fn: (r) => r["_measurement"] == "diskio")
  |> filter(fn: (r) => r["_field"] == "read_time")
  |> group(columns: ["node_ip"], mode:"by")
  |> keep(columns: ["node_ip", "_value"])
  |> first()

Last = from(bucket: "doctorado")
  |> range(start: -5m, stop: now())
  |> filter(fn: (r) => r["_measurement"] == "diskio")
  |> filter(fn: (r) => r["_field"] == "read_time")
  |> group(columns: [ "node_ip"], mode:"by")
  |> keep(columns: ["node_ip", "_value"])
  |> last()

union(tables: [ First, Last])
|> difference()
|> map(fn: (r) => ({r with  _value: math.abs(x: float(v: r._value))}))'


peticiones

influx query 'from(bucket: "doctorado")
  |> range(start: -101m, stop: -1m)
  |> filter(fn: (r) =>
      r._measurement == "nginx" and
      r._field == "requests"
  )
  |> aggregateWindow(every: 3m, fn: max, createEmpty: false)
  |> derivative(unit: 1s, nonNegative: true)
  |> group(columns: ["node"])
  |> rename(columns: {_value: "throughput_rps"})
  |> keep(columns: ["_time", "node", "throughput_rps"])'


MEJORADA

 influx query 'from(bucket: "doctorado")
  |> range(start: -101m, stop: -1m)
  |> filter(fn: (r) => r._measurement == "nginx" and r._field == "requests")
  |> aggregateWindow(every: 3m, fn: last, createEmpty: false)
  |> group(columns: ["node", "_time"])
  |> sum()
  |> group(columns: ["node"])
  |> derivative(unit: 1s, nonNegative: true)
  |> rename(columns: {_value: "throughput_rps"})
  |> keep(columns: ["_time", "node", "throughput_rps"])'




RESPONSE TIME

influx query 'from(bucket: "doctorado")
  |> range(start: -101m, stop: -1m)
  |> filter(fn: (r) => r._measurement == "http_response" and r._field == "response_time")
  |> aggregateWindow(every: 3m, fn: mean, createEmpty: false)
  |> group(columns: ["node"])
  |> rename(columns: {_value: "avg_waiting"})
  |> keep(columns: ["_time", "node", "avg_waiting"])'


NUEVA

influx query 'import "math" from(bucket: "doctorado")
  |> range(start: -101m, stop: -1m)
  |> filter(fn: (r) =>
      r._measurement == "http_response" and
      r._field == "response_time"
  )
  |> group(columns: ["node"])                             // combinar series del nodo
  |> aggregateWindow(every: 3m, fn: mean, createEmpty: false)
  |> rename(columns: {_value: "avg_waiting"})
  |> keep(columns: ["_time", "node", "avg_waiting"])'


